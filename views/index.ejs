<DOCTYPE html>
    <head>
        <title>mini message board | Home</title>
    </head>
    <body>
        <% messages.forEach(message => { %>
            <h2><%= message.text %></h2>
            <h3><%= message.user %></h3>
            <p><%= message.added %></p>
            <hr>
            <br>
        <% }) %>
        <a href="/new">Click to log a new message</a>
    </body>
</html>