<DOCTYPE html>
    <head>
        <title>mini message board | Home</title>
    </head>
    <body>
        <% messages.forEach((message, index) => { %>
            <h2>Hidden Message</h2>
            <h3><%= message.user %></h3>
            <a href="/message/<%= index %>"><button type="button" >read message</button></a>
            <hr>
            <br>
        <% }) %>
        <a href="/new">Click to log a new message</a>
    </body>
</html>